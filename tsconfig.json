{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "lib": ["esnext.array", "esnext", "es2017", "dom", "es5"],
    "module": "esnext",
    "moduleResolution": "node",
    "removeComments": true,
    "strict": true,
    "target": "esnext",
    "tsBuildInfoFile": ".buildcache/default"
  },
  "exclude": [
    "node_modules"
  ]
}
