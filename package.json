{
  "name": "@buckneri/js-lib",
  "version": "0.0.29",
  "description": "Tools for various javascript tasks in the browser",
  "main": "js-lib.js",
  "private": true,
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run tsc && npm run bundle",
    "bundle": "rollup -c",
    "clean": "lerna clean && lerna link && lerna bootstrap",
    "setup": "lerna bootstrap",
    "upload": "npm publish --access public",
    "upload-test": "npm publish --dry-run --access public",
    "test": "jest --config jestconfig.json",
    "tsc": "lerna run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibuckner/js-lib.git"
  },
  "author": "Ivor Buckner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibuckner/js-lib/issues"
  },
  "homepage": "https://github.com/ibuckner/js-lib#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "lerna": "^3.16.4",
    "rollup": "^1.20.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  }
}
